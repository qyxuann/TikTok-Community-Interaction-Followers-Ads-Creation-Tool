{
  "manifest_version": 3,
  "name": "TikTok社区互动-账号关注广告创建工具",
  "version": "1.0",
  "description": "一键创建TikTok社区互动-账号关注广告",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_title": "TikTok社区互动-账号关注广告创建工具"
  },
  "permissions": [
    "storage",
    "tabs",
    "identity"
  ],
  "host_permissions": [
    "https://open.feishu.cn/*",
    "https://business-api.tiktok.com/*",
    "https://ads.tiktok.com/*"
  ],
  "background": {
    "service_worker": "js/background.js",
    "type": "module"
  },
  "key": "",
  "web_accessible_resources": [{
    "resources": [
      "html/popup.html",
      "html/settings.html",
      "html/logs.html",
      "js/*",
      "css/*",
      "images/*",
      "icons/*"
    ],
    "matches": ["<all_urls>"]
  }]
} 